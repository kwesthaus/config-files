partial xkb_types "orville" {
    type "SUPER_ALT_ALPHABETIC" {
        modifiers= Shift+Mod4+Alt;
        map[Shift]= Level2;
        map[Shift+Mod4]= Level2;
        map[Shift+Alt]= Level2;
        map[Shift+Mod4+Alt]= Level2;
        map[Mod4+Alt]= Level3;
        level_name[Level1]= "Base";
        level_name[Level2]= "Shift";
        level_name[Level3]= "SuperAlt";
    };
};
