IdentitiesOnly yes
AddKeysToAgent yes

Host *.labs.overthewire.org
	SendEnv WECHALLTOKEN
	SendEnv WECHALLUSER

Host aur
	Preferredauthentications publickey
	HostName aur.archlinux.org
	User aur
	IdentityFile ~/.ssh/aur/id_ed25519

Host std
        Preferredauthentications publickey
        HostName cse-sl2.coeit.osu.edu
        User westhaus.3
        IdentityFile ~/.ssh/osu/id_ed25519

Host alarm
        Preferredauthentications publickey
        HostName alarm.localdomain
        User alarm

Host jump5472
        Preferredauthentications publickey
        HostName cse-jump-5472.coeit.osu.edu
        User westhaus.3
        IdentityFile ~/.ssh/osu/id_ed25519

Host template5472user
        Preferredauthentications publickey
        HostName 10.4.1.4
        ProxyJump jump5472
        User thompson
        IdentityFile ~/.ssh/cse5472/p1-thompson

Host template5472root
        HostName 10.4.1.4
        ProxyJump jump5472
        User root

Host login5472
        Preferredauthentications publickey
        HostName 10.4.0.7
        ProxyJump jump5472
