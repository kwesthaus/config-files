IdentitiesOnly yes
AddKeysToAgent yes

Host *.labs.overthewire.org
	SendEnv WECHALLTOKEN
	SendEnv WECHALLUSER

Host aur
	Preferredauthentications publickey
	HostName aur.archlinux.org
	User aur
	IdentityFile ~/.ssh/aur/id_ed25519

Host std
        HostName stdlinux.cse.ohio-state.edu
        User westhaus.3

Host alarm-home
        HostName 192.168.1.90
        User alarm

Host alarm-school
