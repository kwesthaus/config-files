IdentitiesOnly yes
AddKeysToAgent yes

Host *.labs.overthewire.org
	SendEnv WECHALLTOKEN
	SendEnv WECHALLUSER

Host aur
	Preferredauthentications publickey
	HostName aur.archlinux.org
	User aur
	IdentityFile ~/.ssh/aur/id_ed25519

